		"@vitejs/plugin-vue": "^1.9.4",
		"eslint": "^8.2.0",
		"eslint-config-marine": "^9.0.6",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-vue": "^8.1.1",
		"husky": "^7.0.4",
		"is-ci": "^3.0.1",
		"prettier": "^2.4.1",
		"typescript": "^4.5.2",
		"vite": "^2.6.14",
		"vite-plugin-pages": "^0.19.0-beta.3",
