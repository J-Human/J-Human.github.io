		"@vitejs/plugin-vue": "^2.0.0",
		"eslint": "^8.4.1",
		"eslint-config-marine": "^9.1.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-vue": "^9.0.0",
		"husky": "^8.0.0",
		"is-ci": "^3.0.1",
		"prettier": "^2.5.1",
		"typescript": "^4.5.3",
		"vite": "^2.7.1",
